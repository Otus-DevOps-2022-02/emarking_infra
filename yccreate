yc compute instance create \
    --name reddit-app \
    --hostname reddit-app \
    --zone ru-central1-a
    --create-boot-disk image-folder-id=standard-images,image-family=ubuntu-1804,size=10GB \
    --platform standard-v3 \
    --cores=2 \
    --core-fraction=20 \
    --memory=4 \
    --preemptible \
    --public-ip true \
    --network-interface subnet-name=default-ru-central1-a,nat-ip-version=ipv4,subnet-id=e9bq2oe6jsaapniab33p \
    --metadata serial-port-enable=1 \
    --metadata-from-file user-data=setting.yaml \
    #--ssh-key ~/.ssh/id_rsa.pub


yc compute instance create --name reddit-app --hostname reddit-app  --zone ru-central1-a --create-boot-disk image-folder-id=standard-images,image-family=ubuntu-1804,size=10GB --platform standard-v3 --cores=2 --core-fraction=20 --memory=4 --preemptible --network-interface subnet-name=mynet,nat-ip-version=ipv4, --metadata serial-port-enable=1   --metadata-from-file user-data=setting.yaml
